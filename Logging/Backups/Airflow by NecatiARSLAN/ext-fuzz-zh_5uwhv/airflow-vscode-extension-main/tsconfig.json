{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "outDir": "out",
    "sourceMap": true,
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false,
    "allowUnreachableCode": true,
    "allowUnusedLabels": true,
    "noEmitOnError": false,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}