{
    "name": "snippet-finder",
    "displayName": "Snippet Finder",
    "description": "Logs available snippets for the active language",
    "version": "0.0.1",
    "publisher": "codyb",
    "engines": {
        "vscode": "^1.75.0"
    },
    "main": "./out/extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "snippet-finder.showSnippets",
                "title": "Show Available Snippets"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.0.0",
        "@types/node-fetch": "^2.6.12",
        "@types/vscode": "^1.75.0",
        "typescript": "^4.0.0"
    },
    "dependencies": {
        "csv-parse": "^5.6.0",
        "node-fetch": "^2.7.0"
    }
}
